{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "tsc -b tsconfig.app.json && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:automated": "vitest run --reporter=verbose --no-watch",
    "test:e2e": "vitest run src/test/e2e/browser-puppeteer.test.ts",
    "test:browser": "npm run test:e2e",
    "deploy": "npm run build && gh-pages -d dist",
    "predeploy": "npm run build"
  },
  "dependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/selenium-webdriver": "^4.35.4",
    "@vitejs/plugin-react": "^5.1.1",
    "chromedriver": "^143.0.3",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "puppeteer": "^24.34.0",
    "selenium-webdriver": "^4.39.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.16"
  }
}
